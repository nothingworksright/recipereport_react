{
  "expo": {
    "name": "Recipe.Report",
    "description": "The Recipe.Report mobile application, built using React-Native",
    "slug": "recipereport",
    "privacy": "unlisted",
    "sdkVersion": "23.0.0",
    "version": "0.0.1",
    "primaryColor": "#607D8B",
    "icon": "icons/icon_512.png",
    "notification": {
      "icon": "icons/icon_48_gs.png",
      "color": "#607D8B",
      "androidMode": "default",
      "androidCollapsedTitle": "#{unread_notifications} - Recipe.Report"
    },
    "loading": {
      "icon": "icons/icon_512.png",
      "exponentIconColor": "blue",
      "exponentIconGrayscale": 1,
      "backgroundColor": "#ECEFF1",
      "hideExponentText": true
    },
    "androidStatusBar": {
      "barStyle": "light-content",
      "backgroundColor": "#607D8B"
    },
    "android": {
      "package": "report.recipe.recipe.report",
      "versionCode": 1,
      "icon": "icons/icon_640.png",
      "splash": {
        "backgroundColor": "#ECEFF1",
        "resizeMode": "contain",
      }
    },
    "splash": {
      "backgroundColor": "#ECEFF1",
      "resizeMode": "contain",
      "image": "icons/icon_640.png"
    }
  }
}
